import Container from "../components/Container.astro";

const seo = {
  title: "Fibras MX | Las Fibras",
  description: "Las Fibras que cotizan en la BMV ...",
};
---

<Layout seo={seo}>
  <Container>
    <div class="home-hero-text__container">
      <h1>Las Fibras</h1>
