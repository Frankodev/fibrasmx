---
import Layout from "../layouts/Page.astro";
import Container from "../components/Container.astro";
import Mensaje from "../components/mensaje.astro";

import Community from '../components/Community.astro'

const seo = {
  title: "Fibras MX | La Comunidad",
  description:
    "La comunidad Fibras MX tiene 2 años de haber nacido como una iniciativa ...",
};
---

<Layout seo={seo}>
  <Container>
    <div class="home-hero-text__container">
      <h1 class="title fade-in">Fibras MX</h1>
    </div>
    <center>
      <p>Bienvenido a la comunidad más grande amantes de las fibras</p>
    </center>
    <Community />
  </Container>
</Layout>
